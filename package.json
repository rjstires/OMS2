{
  "name": "react-slingshot",
  "version": "4.0.0",
  "description": "Starter kit for creating apps with React and Redux",
  "scripts": {
    "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch --fix",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "preinstall": "npm run clean-dist",
    "postinstall": "babel-node tools/build.js",
    "build": "npm run build:html && node server.js"
  },
  "engines": {
    "node": "6.3.0"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "axios": "0.12.0",
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "babel-register": "6.8.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.0",
    "chalk": "^1.1.3",
    "cheerio": "0.20.0",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.2.0",
    "cors": "^2.7.1",
    "crypto-js": "^3.1.6",
    "css-loader": "0.23.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "hammerjs": "2.0.8",
    "jwt-simple": "^0.5.0",
    "knex": "^0.11.7",
    "lodash": "^4.13.1",
    "mandrill-api": "^1.0.45",
    "node-sass": "3.7.0",
    "npm-run-all": "1.8.0",
    "object-assign": "4.1.0",
    "pg": "^6.0.2",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "readdir": "0.0.13",
    "redux": "3.5.2",
    "redux-form": "5.2.5",
    "redux-thunk": "2.1.0",
    "rimraf": "2.5.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "toastr": "^2.1.2",
    "webpack": "1.13.0"
  },
  "devDependencies": {
    "browser-sync": "2.12.5",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-jsx-a11y": "1.0.4",
    "eslint-plugin-react": "5.2.2",
    "isparta": "4.0.0",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.5.3",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "should": "^10.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "sqlite3": "^3.1.4",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-router",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "redux",
    "flux",
    "boilerplate",
    "starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coryhouse/react-slingshot"
  }
}
